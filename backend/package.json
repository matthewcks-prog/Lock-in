{
  "name": "lock-in-backend",
  "version": "1.0.0",
  "description": "Backend API for Lock-in Chrome extension - AI-powered study assistant",
  "main": "index.js",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "start": "node index.js",
    "start:prod": "NODE_ENV=production node index.js",
    "dev": "nodemon index.js",
    "test": "node --test",
    "docker:build": "cd .. && docker build -f backend/Dockerfile -t lock-in-backend .",
    "docker:run": "docker run -p 3000:3000 --env-file ../.env lock-in-backend"
  },
  "keywords": [
    "education",
    "ai",
    "study-assistant"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@sentry/node": "^10.32.1",
    "@supabase/supabase-js": "^2.86.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "ffmpeg-static": "^5.3.0",
    "file-type": "^18.7.0",
    "multer": "^2.0.2",
    "openai": "^4.20.1",
    "pdf-parse": "^1.1.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}
